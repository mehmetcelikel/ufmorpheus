<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY p1 "#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY owl2 "http://www.w3.org/2006/12/owl2#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY sample "http://morpheus.cise.ufl.edu/sample#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://morpheus.cise.ufl.edu/sample#"
     xml:base="http://morpheus.cise.ufl.edu/sample"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:p1="#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl2="http://www.w3.org/2006/12/owl2#"
     xmlns:sample="http://morpheus.cise.ufl.edu/sample#">
    <owl:Ontology rdf:about=""/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://morpheus.cise.ufl.edu/sample#ValueWithinContext -->

    <owl:ObjectProperty rdf:about="#ValueWithinContext"/>
    


    <!-- http://morpheus.cise.ufl.edu/sample#What_output -->

    <owl:ObjectProperty rdf:about="#What_output"/>
    


    <!-- http://morpheus.cise.ufl.edu/sample#Where_input -->

    <owl:ObjectProperty rdf:about="#Where_input"/>
    


    <!-- http://morpheus.cise.ufl.edu/sample#hasModifier -->

    <owl:ObjectProperty rdf:about="#hasModifier"/>
    


    <!-- http://morpheus.cise.ufl.edu/sample#hasModifierList -->

    <owl:ObjectProperty rdf:about="#hasModifierList"/>
    


    <!-- http://morpheus.cise.ufl.edu/sample#hasRealm -->

    <owl:ObjectProperty rdf:about="#hasRealm">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:domain rdf:resource="#QueryClass"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- #hasRank -->

    <owl:DatatypeProperty rdf:about="#hasRank"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://morpheus.cise.ufl.edu/sample#City -->

    <owl:Class rdf:about="#City"/>
    


    <!-- http://morpheus.cise.ufl.edu/sample#CityContext -->

    <owl:Class rdf:about="#CityContext">
        <rdfs:subClassOf rdf:resource="#ContextClass"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ValueWithinContext"/>
                <owl:allValuesFrom rdf:resource="#City"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ValueWithinContext"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://morpheus.cise.ufl.edu/sample#ContextClass -->

    <owl:Class rdf:about="#ContextClass"/>
    


    <!-- http://morpheus.cise.ufl.edu/sample#Hotel -->

    <owl:Class rdf:about="#Hotel"/>
    


    <!-- http://morpheus.cise.ufl.edu/sample#HotelContext -->

    <owl:Class rdf:about="#HotelContext">
        <rdfs:subClassOf rdf:resource="#ContextClass"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ValueWithinContext"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasModifierList"/>
                <owl:allValuesFrom rdf:resource="#HotelContextModifierList"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ValueWithinContext"/>
                <owl:allValuesFrom rdf:resource="#Hotel"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://morpheus.cise.ufl.edu/sample#HotelContextModifierList -->

    <owl:Class rdf:about="#HotelContextModifierList">
        <rdfs:subClassOf rdf:resource="#ModifierList"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasModifier"/>
                <owl:hasValue rdf:resource="#HotelContextModifier2"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasModifier"/>
                <owl:hasValue rdf:resource="#HotelContextModifier1"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://morpheus.cise.ufl.edu/sample#Modifier -->

    <owl:Class rdf:about="#Modifier"/>
    


    <!-- http://morpheus.cise.ufl.edu/sample#ModifierList -->

    <owl:Class rdf:about="#ModifierList"/>
    


    <!-- http://morpheus.cise.ufl.edu/sample#QueryClass -->

    <owl:Class rdf:about="#QueryClass"/>
    


    <!-- http://morpheus.cise.ufl.edu/sample#SSQ -->

    <owl:Class rdf:about="#SSQ"/>
    


    <!-- http://morpheus.cise.ufl.edu/sample#SSQ-0 -->

    <owl:Class rdf:about="#SSQ-0">
        <rdfs:subClassOf rdf:resource="#SSQ"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasRealm"/>
                <owl:allValuesFrom rdf:resource="#Travel"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#What_output"/>
                <owl:allValuesFrom rdf:resource="#HotelContext"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#Where_input"/>
                <owl:allValuesFrom rdf:resource="#CityContext"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://morpheus.cise.ufl.edu/sample#Travel -->

    <owl:Class rdf:about="#Travel"/>
    


    <!-- http://morpheus.cise.ufl.edu/sample#cheapest -->

    <owl:Class rdf:about="#cheapest">
        <rdfs:subClassOf rdf:resource="#Modifier"/>
    </owl:Class>
    


    <!-- http://morpheus.cise.ufl.edu/sample#five-star -->

    <owl:Class rdf:about="#five-star">
        <rdfs:subClassOf rdf:resource="#Modifier"/>
    </owl:Class>
    


    <!-- http://morpheus.cise.ufl.edu/samplehttp://www.w3.org/2002/07/owl#Thing -->

    <owl:Class rdf:about="&owl;Thing"/>
    


    <!-- http://www.w3.org/2002/07/owl#Thing -->

    <owl:Class rdf:about="&owl;Thing"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- #HotelContextModifier1 -->

    <cheapest rdf:about="#HotelContextModifier1">
        <p1:hasRank rdf:datatype="&xsd;integer">1</p1:hasRank>
    </cheapest>
    


    <!-- #HotelContextModifier2 -->

    <five-star rdf:about="#HotelContextModifier2">
        <p1:hasRank rdf:datatype="&xsd;integer">2</p1:hasRank>
    </five-star>
</rdf:RDF>
