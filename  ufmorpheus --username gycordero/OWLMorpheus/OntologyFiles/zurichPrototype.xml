            <owl:Restriction>
                <owl:onProperty rdf:resource="#ValueWithinContext"/>
                <owl:allValuesFrom rdf:resource="#Hotel"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ValueWithinContext"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#Modifier1"/>
                <owl:hasValue rdf:resource="#cheapest"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#Modifier2"/>
                <owl:hasValue rdf:resource="#five-star"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


